            {q:"Content Ã¼retimi iÃ§in aÅŸaÄŸÄ±daki formatlarÄ± sÄ±rala:",options:[{text:"YazÄ±lÄ± iÃ§erik (blog, makale)",type:"I"},{text:"Video Ã§ekimi ve kurgusu",type:"A"},{text:"Sosyal medya community management",type:"S"},{text:"Ä°Ã§erik takvimleri ve planlama",type:"C"}]},
            {q:"GSC iÃ§in aÅŸaÄŸÄ±daki gÃ¶revleri sÄ±rala:",options:[{text:"KatÄ±lÄ±mcÄ± ekiplere mentoring yapmak",type:"S"},{text:"YarÄ±ÅŸma sÃ¼reÃ§lerini organize etmek",type:"C"},{text:"Projelerin teknik kalitesini deÄŸerlendirmek",type:"I"},{text:"YarÄ±ÅŸmayÄ± tanÄ±tmak ve sponsorlarla gÃ¶rÃ¼ÅŸmek",type:"E"}]},
            {q:"SatÄ±ÅŸ sÃ¼recinde aÅŸaÄŸÄ±daki rolleri sÄ±rala:",options:[{text:"MÃ¼ÅŸterilerle gÃ¶rÃ¼ÅŸmeler ve ikna",type:"E"},{text:"SatÄ±ÅŸ verilerini analiz etmek",type:"I"},{text:"CRM sistemini yÃ¶netmek ve takip",type:"C"},{text:"SatÄ±ÅŸ materyallerini tasarlamak",type:"A"}]},
            {q:"Bir krizde (Ã¶rn: son dakika deÄŸiÅŸiklikleri) aÅŸaÄŸÄ±daki yaklaÅŸÄ±mlarÄ± sÄ±rala:",options:[{text:"HÄ±zlÄ±ca pratik Ã§Ã¶zÃ¼m bulup uygulamak",type:"R"},{text:"Durumu analiz edip en iyi Ã§Ã¶zÃ¼mÃ¼ bulmak",type:"I"},{text:"Ekibin moralini yÃ¼ksek tutmak",type:"S"},{text:"Krizi fÄ±rsata Ã§evirmek iÃ§in yaratÄ±cÄ± dÃ¼ÅŸÃ¼nmek",type:"A"}]}
        ];
        let state={stage:'welcome',currentQuestion:0,answers:{},rankingAnswers:{},scores:{R:0,I:0,A:0,S:0,E:0,C:0}};
        function render(){const app=document.getElementById('app');if(state.stage==='welcome'){app.innerHTML=`<div class="min-h-screen flex items-center justify-center p-4"><div class="max-w-2xl w-full bg-white rounded-3xl shadow-2xl p-12 fade-in"><div class="text-center mb-8"><div class="w-20 h-20 bg-gradient-to-br from-blue-500 to-purple-600 rounded-full mx-auto mb-6 flex items-center justify-center"><span class="text-white text-3xl font-bold">YGA</span></div><h1 class="text-4xl font-bold text-gray-900 mb-4">GÃ¶nÃ¼llÃ¼ Ä°lgi AlanlarÄ± Testi</h1><p class="text-xl text-gray-600 mb-8">YGA'da hangi rollerde en Ã§ok keyif alacaÄŸÄ±nÄ± keÅŸfet</p></div><div class="bg-gradient-to-br from-blue-50 to-purple-50 rounded-2xl p-8 mb-8"><h2 class="text-lg font-semibold text-gray-900 mb-4">Ã–nemli:</h2><ul class="space-y-3 text-gray-700"><li class="flex items-start"><svg class="w-5 h-5 text-blue-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/></svg><span>Neyi iyi yaptÄ±ÄŸÄ±nÄ± deÄŸil, neyi yapmaktan zevk aldÄ±ÄŸÄ±nÄ± dÃ¼ÅŸÃ¼n</span></li><li class="flex items-start"><svg class="w-5 h-5 text-blue-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/></svg><span>BaÅŸkalarÄ±nÄ±n senden ne beklediÄŸini deÄŸil, sen ne istediÄŸini iÅŸaretle</span></li><li class="flex items-start"><svg class="w-5 h-5 text-blue-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/></svg><span>'DoÄŸru' cevap yok, sadece senin cevabÄ±n var</span></li><li class="flex items-start"><svg class="w-5 h-5 text-blue-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/></svg><span>Ä°lk iÃ§gÃ¼dÃ¼nÃ¼ takip et, Ã§ok dÃ¼ÅŸÃ¼nme</span></li></ul></div><div class="flex items-center justify-between text-sm text-gray-600 mb-8"><div class="flex items-center"><div class="w-10 h-10 rounded-full bg-blue-100 flex items-center justify-center mr-3"><span class="text-blue-600 font-semibold">66</span></div><span>Soru</span></div><div class="flex items-center"><div class="w-10 h-10 rounded-full bg-purple-100 flex items-center justify-center mr-3"><span class="text-purple-600 font-semibold">15</span></div><span>Dakika</span></div></div><button onclick="startTest()" class="w-full bg-gradient-to-r from-blue-600 to-purple-600 text-white text-lg font-semibold py-4 rounded-2xl hover:from-blue-700 hover:to-purple-700 transition-all duration-200 shadow-lg hover:shadow-xl">Teste BaÅŸla</button></div></div>`;}else if(state.stage==='test'){renderTest();}else if(state.stage==='ranking'){renderRanking();}else if(state.stage==='results'){renderResults();}}
        function renderTest(){const question=forcedChoiceQuestions[state.currentQuestion];const progress=(state.currentQuestion/forcedChoiceQuestions.length)*100;document.getElementById('app').innerHTML=`<div class="min-h-screen flex items-center justify-center p-4"><div class="max-w-3xl w-full"><div class="mb-8"><div class="flex items-center justify-between mb-3"><span class="text-sm font-medium text-gray-600">Soru ${state.currentQuestion+1} / ${forcedChoiceQuestions.length}</span><span class="text-sm font-medium text-blue-600">BÃ¶lÃ¼m 1: SeÃ§im SorularÄ±</span></div><div class="w-full h-2 bg-gray-200 rounded-full overflow-hidden"><div class="h-full bg-gradient-to-r from-blue-600 to-purple-600 progress-bar" style="width: ${progress}%"></div></div></div><div class="bg-white rounded-3xl shadow-2xl p-8 md:p-12 fade-in"><h2 class="text-2xl md:text-3xl font-bold text-gray-900 mb-8 leading-relaxed">${question.q}</h2><div class="space-y-4">${question.options.map((option,idx)=>`<button onclick="handleAnswer('${option.type}')" class="w-full text-left p-6 rounded-2xl border-2 transition-all duration-200 hover:border-blue-500 hover:shadow-lg group ${state.answers[state.currentQuestion]===option.type?'border-blue-600 bg-blue-50 shadow-md':'border-gray-200 bg-white hover:bg-gray-50'}"><div class="flex items-start"><div class="w-6 h-6 rounded-full border-2 flex items-center justify-center mr-4 mt-0.5 flex-shrink-0 transition-colors ${state.answers[state.currentQuestion]===option.type?'border-blue-600 bg-blue-600':'border-gray-300 group-hover:border-blue-500'}">${state.answers[state.currentQuestion]===option.type?'<div class="w-2 h-2 bg-white rounded-full"></div>':''}</div><span class="text-lg text-gray-800 leading-relaxed">${option.text}</span></div></button>`).join('')}</div>${state.currentQuestion>0?`<button onclick="previousQuestion()" class="mt-8 text-gray-600 hover:text-gray-900 font-medium transition-colors">â† Ã–nceki Soru</button>`:''}</div></div></div>`;}
        function renderRanking(){const allComplete=checkAllRankingsComplete();document.getElementById('app').innerHTML=`<div class="min-h-screen p-4 py-12"><div class="max-w-4xl mx-auto"><div class="mb-8"><div class="flex items-center justify-between mb-3"><span class="text-sm font-medium text-gray-600">BÃ¶lÃ¼m 2: SÄ±ralama SorularÄ±</span><span class="text-sm font-medium text-purple-600">Son AÅŸama</span></div><div class="w-full h-2 bg-gray-200 rounded-full overflow-hidden"><div class="h-full bg-gradient-to-r from-purple-600 to-pink-600 w-full"></div></div></div><div class="bg-white rounded-3xl shadow-2xl p-8 md:p-12 mb-8 fade-in"><h2 class="text-2xl md:text-3xl font-bold text-gray-900 mb-4">Son AÅŸama: Aktiviteleri SÄ±rala</h2><p class="text-gray-600 mb-8 text-lg">Her soruda aktiviteleri <span class="font-semibold text-purple-600">1-4 arasÄ± sÄ±rala</span>. (1=En Ã§ok keyif alÄ±rÄ±m, 4=En az keyif alÄ±rÄ±m)</p><div class="space-y-12">${rankingQuestions.map((question,qIdx)=>`<div class="border-b border-gray-200 pb-8 last:border-b-0"><h3 class="text-lg font-semibold text-gray-900 mb-6">${qIdx+1}. ${question.q}</h3><div class="space-y-4">${question.options.map((option,optIdx)=>`<div class="bg-gray-50 rounded-xl p-5"><div class="flex items-center justify-between"><span class="text-gray-800 flex-1 pr-4">${option.text}</span><div class="flex gap-2">${[1,2,3,4].map(rank=>`<button onclick="handleRanking(${qIdx}, ${optIdx}, ${rank})" class="w-12 h-12 rounded-xl font-semibold transition-all duration-200 ${(state.rankingAnswers[qIdx]&&state.rankingAnswers[qIdx][optIdx]===rank)?'bg-gradient-to-br from-purple-600 to-pink-600 text-white shadow-lg scale-110':'bg-white text-gray-600 border-2 border-gray-300 hover:border-purple-500 hover:text-purple-600'}">${rank}</button>`).join('')}</div></div></div>`).join('')}</div></div>`).join('')}</div></div><button onclick="calculateResults()" ${!allComplete?'disabled':''} class="w-full text-lg font-semibold py-5 rounded-2xl transition-all duration-200 shadow-lg flex items-center justify-center ${allComplete?'bg-gradient-to-r from-purple-600 to-pink-600 text-white hover:from-purple-700 hover:to-pink-700 hover:shadow-xl':'bg-gray-300 text-gray-500 cursor-not-allowed'}">${allComplete?'SonuÃ§larÄ± GÃ¶r â†’':'LÃ¼tfen tÃ¼m sorularÄ± tamamla'}</button></div></div>`;}
        function renderResults(){const typeInfo={R:{name:"Realistic",desc:"Pratik, elle yapma, teknik"},I:{name:"Investigative",desc:"Analitik, araÅŸtÄ±rmacÄ±, meraklÄ±"},A:{name:"Artistic",desc:"YaratÄ±cÄ±, Ã¶zgÃ¼n, ifade odaklÄ±"},S:{name:"Social",desc:"Ä°nsancÄ±l, yardÄ±msever, empatik"},E:{name:"Enterprising",desc:"GiriÅŸimci, lider, ikna edici"},C:{name:"Conventional",desc:"Organize, sistemli, detaycÄ±"}};const topTypes=Object.entries(state.scores).sort((a,b)=>b[1]-a[1]).slice(0,3);const maxScore=102;const roles=getRoleRecommendations();document.getElementById('app').innerHTML=`<div class="min-h-screen p-4 py-12"><div class="max-w-4xl mx-auto"><div class="bg-white rounded-3xl shadow-2xl p-8 md:p-12 mb-8 fade-in"><div class="text-center mb-12"><div class="w-24 h-24 bg-gradient-to-br from-green-500 to-emerald-600 rounded-full mx-auto mb-6 flex items-center justify-center"><svg class="w-12 h-12 text-white" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/></svg></div><h1 class="text-4xl font-bold text-gray-900 mb-4">SonuÃ§larÄ±n HazÄ±r!</h1><p class="text-xl text-gray-600">Ä°ÅŸte senin YGA ilgi alanÄ± profilin</p></div><div class="mb-12"><h2 class="text-2xl font-bold text-gray-900 mb-6">ğŸ“Š RIASEC Profil SkorlarÄ±n</h2><div class="space-y-4">${Object.entries(state.scores).map(([type,score])=>{const info=typeInfo[type];const percentage=Math.round((score/maxScore)*100);return`<div><div class="flex items-center justify-between mb-2"><div><span class="font-semibold text-gray-900 text-lg">${type} - ${info.name}</span><span class="text-gray-600 text-sm ml-3">${info.desc}</span></div><span class="font-bold text-2xl text-gray-900">${percentage}%</span></div><div class="w-full h-4 bg-gray-200 rounded-full overflow-hidden"><div class="h-full bg-gradient-to-r from-blue-600 to-purple-600 progress-bar" style="width: ${percentage}%"></div></div></div>`;}).join('')}</div></div><div class="bg-gradient-to-br from-blue-50 to-purple-50 rounded-2xl p-8 mb-12"><h2 class="text-2xl font-bold text-gray-900 mb-4">ğŸ¯ Senin Profil Tipin</h2><div class="text-center mb-6"><div class="inline-block bg-gradient-to-r from-blue-600 to-purple-600 text-white px-8 py-4 rounded-2xl text-3xl font-bold mb-4">${topTypes[0][0]}-${topTypes[1][0]}</div><p class="text-xl text-gray-700 font-medium">${typeInfo[topTypes[0][0]].name} - ${typeInfo[topTypes[1][0]].name}</p></div><div class="grid md:grid-cols-3 gap-4 mt-6">${topTypes.map(([type,score],idx)=>`<div class="bg-white rounded-xl p-4 text-center"><div class="text-sm text-gray-600 mb-1">${idx===0?'ğŸ¥‡ En YÃ¼ksek':idx===1?'ğŸ¥ˆ Ä°kinci':'ğŸ¥‰ ÃœÃ§Ã¼ncÃ¼'}</div><div class="text-2xl font-bold text-gray-900">${type}</div><div class="text-sm text-gray-600">${Math.round((score/maxScore)*100)}%</div></div>`).join('')}</div></div><div class="mb-12"><h2 class="text-2xl font-bold text-gray-900 mb-6">â­ Senin Ä°Ã§in En Uygun YGA Rolleri</h2><div class="space-y-4">${roles.map((role,idx)=>`<div class="bg-gradient-to-r from-white to-gray-50 border-2 border-gray-200 rounded-2xl p-6 hover:border-blue-500 hover:shadow-lg transition-all duration-200"><div class="flex items-center"><div class="w-12 h-12 bg-gradient-to-br from-blue-600 to-purple-600 rounded-full flex items-center justify-center text-white font-bold text-xl mr-4 flex-shrink-0">${idx+1}</div><div><h3 class="text-xl font-bold text-gray-900">${role}</h3><p class="text-gray-600">${idx===0?'En uygun rol - Kuvvetli yÃ¶nlerinle tam uyumlu':idx===1?'Ã‡ok uygun - Ä°lgi alanlarÄ±na Ã§ok yakÄ±n':'Uygun - GeliÅŸim iÃ§in harika fÄ±rsat'}</p></div></div></div>`).join('')}</div></div><div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-2xl p-8"><h2 class="text-2xl font-bold text-gray-900 mb-4">ğŸ’¡ Bir Sonraki AdÄ±m</h2><ul class="space-y-3 text-gray-700 text-lg"><li class="flex items-start"><svg class="w-6 h-6 text-green-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"/></svg><span>101 KampÄ±'nda mentorÃ¼nle bu sonuÃ§larÄ± konuÅŸ</span></li><li class="flex items-start"><svg class="w-6 h-6 text-green-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"/></svg><span>Ä°lk projende <strong>${roles[0]}</strong> alanÄ±nÄ± dene</span></li><li class="flex items-start"><svg class="w-6 h-6 text-green-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"/></svg><span>Kendini gÃ¶zlemle ve geliÅŸtir!</span></li></ul></div></div><button onclick="location.reload()" class="w-full bg-gray-200 text-gray-700 text-lg font-semibold py-4 rounded-2xl hover:bg-gray-300 transition-all duration-200">Testi Yeniden BaÅŸlat</button></div></div>`;}
        function startTest(){state.stage='test';render();}
        function handleAnswer(type){state.answers[state.currentQuestion]=type;if(state.currentQuestion<forcedChoiceQuestions.length-1){setTimeout(()=>{state.currentQuestion++;render();},200);}else{setTimeout(()=>{state.stage='ranking';render();},200);}}
        function previousQuestion(){if(state.currentQuestion>0){state.currentQuestion--;render();}}
        function handleRanking(qIdx,optIdx,rank){if(!state.rankingAnswers[qIdx])state.rankingAnswers[qIdx]={};state.rankingAnswers[qIdx][optIdx]=rank;render();}
        function checkAllRankingsComplete(){return rankingQuestions.every((_,qIdx)=>{const rankings=state.rankingAnswers[qIdx];if(!rankings)return false;const values=Object.values(rankings);return values.length===4&&new Set(values).size===4&&values.every(v=>v>=1&&v<=4);});}
        function calculateResults(){const newScores={R:0,I:0,A:0,S:0,E:0,C:0};Object.values(state.answers).forEach(type=>{newScores[type]+=1;});Object.entries(state.rankingAnswers).forEach(([qIndex,rankings])=>{const question=rankingQuestions[parseInt(qIndex)];Object.entries(rankings).forEach(([optIndex,rank])=>{const option=question.options[parseInt(optIndex)];const points=5-parseInt(rank);newScores[option.type]+=points;});});state.scores=newScores;state.stage='results';render();}
        function getRoleRecommendations(){const topTypes=Object.entries(state.scores).sort((a,b)=>b[1]-a[1]).slice(0,3);const primary=topTypes[0][0];const secondary=topTypes[1][0];const roleMap={SI:['Bilim SeferberliÄŸi','Program Development','People & Culture'],IS:['Bilim SeferberliÄŸi','Program Development','Product'],IA:['Product','Technology','Content'],AI:['Product','Content','Technology'],EA:['GÃ¶nÃ¼llendirin (Marketing)','Global Expansion','Content'],AE:['GÃ¶nÃ¼llendirin (Marketing)','Content','Global Expansion'],ES:['People & Culture','Global Expansion','GSC'],SE:['People & Culture','Bilim SeferberliÄŸi','GSC'],EI:['Global Expansion','SatÄ±ÅŸ','GSC'],IE:['Product','Global Expansion','Sustainability'],IC:['Technology','Product','Sustainability'],CI:['Technology','Product','Program Development'],SC:['People & Culture','Bilim SeferberliÄŸi','Program Development'],CS:['People & Culture','Program Development','Content'],EC:['Global Expansion','SatÄ±ÅŸ','GSC'],CE:['Product','Program Development','Technology'],RI:['Technology','Bilim SeferberliÄŸi','Product'],IR:['Technology','Product','Sustainability'],RA:['Content','Technology','Bilim SeferberliÄŸi'],AR:['Content','Product','Technology'],RS:['Bilim SeferberliÄŸi','Technology','Program Development'],SR:['Bilim SeferberliÄŸi','People & Culture','Program Development'],RC:['Technology','Product','Bilim SeferberliÄŸi'],CR:['Technology','Product','Program Development'],RE:['SatÄ±ÅŸ','Global Expansion','Technology'],ER:['Global Expansion','SatÄ±ÅŸ','GSC'],AS:['Content','People & Culture','GÃ¶nÃ¼llendirin'],SA:['People & Culture','Content','Bilim SeferberliÄŸi'],AC:['Content','Product','GÃ¶nÃ¼llendirin'],CA:['Content','Product','Technology']};return roleMap[primary+secondary]||['Product','Program Development','Content'];}
        render();
    </script>
</body>
